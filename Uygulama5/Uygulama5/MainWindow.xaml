<Window x:Class="Uygulama5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uygulama5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <DatePicker x:Name="DpTarih" />
            <DataGrid x:Name="GridBilgi" Height="300"/>
            <TextBox x:Name="Tb1" FontSize="26"/>
            <TextBox x:Name="Tb2" FontSize="26"/>
            <Button Content="Tıkla" Click="Button_Click" Width="350"/>
        </StackPanel>
    </Grid>
</Window>
